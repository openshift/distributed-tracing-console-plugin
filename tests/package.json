{
  "name": "distributed-tracing-console-plugin",
  "version": "0.1.0",
  "private": true,
  "repository": "git@github.com:openshift/distributed-tracing-console-plugin.git",
  "license": "Apache-2.0",
  "scripts": {
    "lint": "eslint ./*.{js,ts} ./src ./cypress ./tests && stylelint \"src/**/*.css\" --allow-empty-input",
    "lint-fix": "eslint ./*.{js,ts} ./src ./cypress ./tests --fix && stylelint \"src/**/*.css\" --allow-empty-input --fix",
    "test-cypress-console": "./node_modules/.bin/cypress open --browser chrome",
    "test-cypress-console-headless": "node --max-old-space-size=4096 ./node_modules/.bin/cypress run --browser chrome --headless",
    "delete:reports": "rm -rf gui_test_screenshots/* || true",
    "clean": "rm -rf node_modules",
    "prettier-all": "prettier --write '**/*.{js,jsx,ts,tsx,json}'"
  },
  "dependencies": {
    "@openshift-console/dynamic-plugin-sdk": "1.0.0",
    "@openshift-console/dynamic-plugin-sdk-webpack": "1.0.2",
